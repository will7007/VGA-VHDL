% multiple-line comment
multiple-line comment %
-- single-line comment
DEPTH = 4096; -- The size of memory in words
WIDTH = 3; -- The size of data in bits 
ADDRESS_RADIX = HEX; -- The radix for address values 
DATA_RADIX = DEC; -- The radix for data values 
CONTENT -- start of (address : data pairs) 
BEGIN
--[00..1FF] : 0;
--[1FF..3FE] : 1;
--[3FE..5FD] : 2;
--[5FD..7FC] : 3;
--[7FC..9FB] : 4;
--[9FB..BFA] : 5;
--[BFA..DF9] : 6;
--[DF9..FFF] : 7;
[00..FFF] : 1 2 4;
END;